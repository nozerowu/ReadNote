
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
    <script src="~/Scripts/vue.min.js"></script>
</head>
<body>
<div id="test">
    <ul>
        <li v-for="tab in tabs">
            {{ tab.text }}
        </li>
    </ul>
</div>
</body>
<script>
    new Vue({
        el: '#test',
        data: {
            tabs: [
                { text: '巴士' },
                { text: '快车' },
                { text: '专车' },
                { text: '顺风车' },
                { text: '出租车' },
                { text: '代驾' }
            ]
        }
    })
</script>
</html>
